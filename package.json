{
  "name": "upload-s3",
  "version": "1.0.0",
  "description": "S3 file upload and viewing with presigned URLs - supports images and videos",
  "main": "server.ts",
  "scripts": {
    "dev": "bun run --watch server.ts",
    "start": "bun run server.ts",
    "build": "bun build ./client/index.html --outdir ./dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.490.0",
    "bun": "latest"
  },
  "devDependencies": {
    "@types/node": "^20.10.0"
  }
} 